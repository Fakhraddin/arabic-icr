function  Samples = ReadLetterPositionSamples( LetterPositionFolder)

LetterPositionFolderList = dir(LetterPositionFolder);
j=0;
for i = 3:length(LetterPositionFolderList)
    current_object = LetterPositionFolderList(i);
    if (~strcmp(current_object.name,'.svn') && isdir = 0 && )
        j=j+1;
        FileName = current_object.name;
        res = dlmread([LetterPositionFolder,'\',FileName]);
        Samples(j) = {res};
    end
end
end
